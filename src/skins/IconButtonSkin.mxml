<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="up" />
		<s:State name="full" />
		<s:State name="half" />
		<s:State name="warning" />
	</s:states>
	
	<!-- SkinParts
	name=iconDisplay, type=spark.primitives.BitmapImage, required=false
	name=labelDisplay, type=spark.core.IDisplayText, required=false
	-->

	<s:Rect id="fill" left="1" right="1" top="1" bottom="1" >
		<s:fill>
			<s:BitmapFill  source.down="@Embed('com/purplemedia/components/date/asset/calendar_orange.png')"  source.disabled="@Embed('com/purplemedia/components/date/asset/calendar.png')"	 alpha.down="0.8"  alpha.disabled="0.2" alpha.full="0.9" 
						   source.up="@Embed('com/purplemedia/components/date/asset/calendar.png')"		source.warning="@Embed('com/purplemedia/components/date/asset/calendar_red.png')"	alpha.warning="0.5"	   />
		</s:fill>
	</s:Rect>

	
	
</s:Skin>
